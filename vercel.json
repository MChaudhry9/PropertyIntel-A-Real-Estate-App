{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/react"
      },
      {
        "src": "server/api/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/api/complaints.py"
      },
      {
        "src": "/(.*)",
        "dest": "/client/$1"
      }
    ]
  }
  